includes:
    - ./vendor/larastan/larastan/extension.neon

parameters:
    level: 5
    paths:
        - app
        - modules
    
    excludePaths:
        - bootstrap
        - storage
        - vendor
        - node_modules
        - public
    
    checkMissingIterableValueType: false
    checkGenericClassInNonGenericObjectType: false
    
    ignoreErrors:
        - '#PHPDoc tag @var#'
        - '#Access to an undefined property#'
    
    tmpDir: storage/phpstan